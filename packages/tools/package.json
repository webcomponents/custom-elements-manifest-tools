{
  "name": "@webcomponents/custom-elements-manifest-tools",
  "private": true,
  "version": "0.0.0",
  "description": "Utilities for working with Custom Elements Manifest files",
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --pretty",
      "files": [
        "tsconfig.json",
        "../../tsconfig.base.json",
        "src/**/*.ts"
      ],
      "output": [
        "index.js",
        "index.js.map",
        "index.d.ts",
        "index.d.ts.map",
        "lib/",
        "test/",
        "tsconfig.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    }
  }
}
